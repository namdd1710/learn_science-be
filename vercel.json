{
  "version": 2,
  "builds": [
    {
      "src": "src/app.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/*.js",
      "use": "@vercel/edge"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/app.js"
    },
    {
      "src": "/(.*)",
      "dest": "src/app.js"
    }
  ]
}
